
public class LexicalAnalyzer {
	
	private interface checkState{
		void check();
	}
	
	private checkState[] states = new checkState[] {
			
	};
	
	public String tokenize(String code) {
		for(int i = 0; i < code.length(); i++) {
			switch(code.charAt(i)) {
			case ' ':
				break;
			}
		}
		return "";
	}
	
	private void state1() {
		
	}
	
	private void state2() {
		
	}
	
	private void state3() {
		
	}
}
